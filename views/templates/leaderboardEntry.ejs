<tr class="border-b hover:bg-zinc-800 border-zinc-700 text-center">
    <th scope="row" class="px-3 py-2 font-semibold text-zinc-100 whitespace-nowrap" >
        <div class="flex">
            <img src="<%= userEntry.profilePictureUrl ? userEntry.profilePictureUrl : `images/defaultProfilePicture.webp` %>" class="w-6 h-6 rounded-full">
            <p class="ml-2"><%= userEntry.username %></p>
        </div>
    </th>
    <td class="px-3 py-2">
        <p><%= userEntry.winCount + userEntry.loseCount !== 0 ? parseFloat(userEntry.winCount / (userEntry.winCount + userEntry.loseCount) * 100).toFixed(1) : 0 %>%</p>
    </td>
    <td class="px-3 py-2">
        <p><%= userEntry.winCount + userEntry.loseCount %></p>
    </td>
</tr>